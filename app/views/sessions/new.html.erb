<h1>Log in here</h1>


<%= form_for(:user, url: signin_path) do |f| %>
    <div class="field">
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </div>
    <div class="field">
        <%= f.label :password %><br>
        <%= f.password_field :password %>
    </div>
    <div class="field">
        <%= f.submit "Sign In" %><br>
    </div>
<% end %>
Or <%= link_to 'Sign up', new_user_path %>

